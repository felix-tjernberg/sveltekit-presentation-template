{
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 4,
    "semi": false,
    "bracketSameLine": true,
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
